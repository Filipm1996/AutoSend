<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.autosend">
    <uses-permission android:name="android.permission.SEND_SMS" />
    <application
        android:name=".activities.activities.AutoSendApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_baseline_face_retouching_natural_24"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity
            android:name=".activities.activities.activities.SettingsActivity"
            android:exported="false" />
        <activity
            android:name=".activities.activities.activitie.CalendarActivity"
            android:screenOrientation="landscape"
            android:exported="false" />
        <activity
            android:name=".activities.activities.activities.BeautyTreatmentsActivity"
            android:exported="false" />
        <activity android:name="layout.ActivityMonthlyIncome"
            android:exported="true"/>
        <activity
            android:name=".activities.activities.activities.ContactBook"
            android:exported="false" />
        <activity
            android:name=".activities.activities.activities.MainActivity"
            android:exported="true">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>